<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        card_view:cardCornerRadius="2dp"
        card_view:cardElevation="1dp"
        card_view:cardUseCompatPadding="true"
        card_view:cardBackgroundColor="#ffffff"
        android:id="@+id/cvListUser">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tvLabelKode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/spiner_base_color"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="Kode"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvKode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/menu_label"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="-"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvLabelNama"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/spiner_base_color"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="Nama"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvNama"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/menu_label"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="-"
                    android:singleLine="false" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tvLabelKota"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/spiner_base_color"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="Kota"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvKota"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/menu_label"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="-"
                    android:singleLine="false" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tvLabelLatitude"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/spiner_base_color"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="Latitude"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvLatitude"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/menu_label"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="-"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvLabelLongitude"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/spiner_base_color"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="Longitude"
                    android:singleLine="false" />

                <TextView
                    android:id="@+id/tvLongitude"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/menu_label"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/lato_medium"
                    tools:text="-"
                    android:singleLine="false" />
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>